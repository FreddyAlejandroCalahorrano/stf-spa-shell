{
  "name": "shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local",
    "start:dev": "ng serve -c dev",
    "start:cert": "ng serve -c cert",
    "start:prod": "ng serve -c production",
    "build": "ng build -c local",
    "build:production": "ng build -c production --base-href /staffingshell/ --deploy-url /staffingshell/ && mv dist/staffingshell/index.html ./dist",
    "build:cert": "ng build -c cert",
    "build:development": "ng build -c dev --base-href /staffingshell/ --deploy-url /staffingshell/ && mv dist/staffingshell/index.html ./dist",
    "build:staging": "ng build -c cert --base-href /staffingshell/ --deploy-url /staffingshell/ && mv dist/staffingshell/index.html ./dist",
    "watch": "ng build --watch --configuration development",
    "test": "npx jest",
    "test:watch": "npx jest --watchAll",
    "test:coverage": "npx jest --coverage",
    "coverage": "npx jest --watch=false --coverage",
    "test:deploy": "npx jest --watch=false --coverage",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@azure/msal-angular": "^2.1.0",
    "@azure/msal-browser": "^2.21.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@pichincha/angular-sdk": "^1.0.17",
    "@pichincha/bb-commons": "^1.0.15",
    "@pichincha/typescript-sdk": "^1.0.23",
    "ngx-build-plus": "^12.2.0",
    "bootstrap": "^5.1.3",
    "lodash": "^4.17.21",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "latest",
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.5",
    "@angular/compiler-cli": "~12.2.0",
     "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "@types/jest": "27.4.1",
    "jest": "27.4.1",
    "eslint": "^7.26.0",
          "karma-mocha-reporter": "^2.2.5",
    "typescript": "~4.3.5"
  }
}
